<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
<TableSelectDialog id="grpSearchHelp" noDataText="No Data Found" title="Storage Location" search="handleSlocSearch" growing="true" growingThreshold="15" confirm="handleSlocClose" cancel="handleSlocClose" items="{slocSearch>/d/results}">
        <ColumnListItem>
            <cells>
                <Text text="{slocSearch>Lgort}"/>
            </cells>
        </ColumnListItem>
        <columns>
            <Column>
                <header>
                    <Text text="Storage Location"/>
                </header>
            </Column>
        </columns>
    </TableSelectDialog> 

</core:FragmentDefinition>